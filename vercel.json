{
  "version": 2,
  "builds": [
    {
      "src": "generate-site.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "buildCommand": "node generate-site.js",
  "outputDirectory": "dist"
}

